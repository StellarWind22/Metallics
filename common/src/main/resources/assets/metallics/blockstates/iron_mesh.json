{
  "multipart": [
    {
      "apply": {
        "model": "metallics:block/iron_mesh_vert"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_edge"
      },
      "when": {
        "north": "false",
        "south": "false",
        "east":  "false",
        "west":  "false"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_edge"
      },
      "when": {
        "north": "false",
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_edge_alt"
      },
      "when": {
        "east": "false",
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_edge_alt",
        "y": 90
      },
      "when": {
        "south": "false",
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "metallics:block/iron_mesh_edge",
        "y": 270
      },
      "when": {
        "west": "false",
        "east": "true"
      }
    }
  ]
}