{
  "multipart": [
    {
      "apply": { "model": "metallics:block/iron_mesh_vert" }
    },

    { "when": { "north": "true" }, "apply": { "model": "metallics:block/iron_mesh_side" } },
    { "when": { "east": "true" },  "apply": { "model": "metallics:block/iron_mesh_side", "y": 90 } },
    { "when": { "south": "true" }, "apply": { "model": "metallics:block/iron_mesh_side_alt" } },
    { "when": { "west": "true" },  "apply": { "model": "metallics:block/iron_mesh_side_alt", "y": 90 } },

    {
      "when": { "north": "false", "south": "false", "east": "false", "west": "false" },
      "apply": { "model": "metallics:block/iron_mesh_post" }
    },

    {
      "when": { "north": "false", "south": "true", "east": "false", "west": "false" },
      "apply": { "model": "metallics:block/iron_mesh_edge" }
    },
    {
      "when": { "east": "false", "west": "true", "north": "false", "south": "false" },
      "apply": { "model": "metallics:block/iron_mesh_edge_alt" }
    },
    {
      "when": { "south": "false", "north": "true", "east": "false", "west": "false" },
      "apply": { "model": "metallics:block/iron_mesh_edge_alt", "y": 90 }
    },
    {
      "when": { "west": "false", "east": "true", "north": "false", "south": "false" },
      "apply": { "model": "metallics:block/iron_mesh_edge", "y": 270 }
    },

    {
      "when": { "north": "false", "south": "true", "east": "true", "west": "true" },
      "apply": { "model": "metallics:block/iron_mesh_edge" }
    },
    {
      "when": { "south": "false", "north": "true", "east": "true", "west": "true" },
      "apply": { "model": "metallics:block/iron_mesh_edge_alt", "y": 90 }
    },
    {
      "when": { "east": "false", "west": "true", "north": "true", "south": "true" },
      "apply": { "model": "metallics:block/iron_mesh_edge_alt" }
    },
    {
      "when": { "west": "false", "east": "true", "north": "true", "south": "true" },
      "apply": { "model": "metallics:block/iron_mesh_edge", "y": 270 }
    },

    {
      "when": { "north": "true", "south": "true", "east": "false", "west": "false" },
      "apply": { "model": "metallics:block/iron_mesh_center", "y": 90 }
    },
    {
      "when": { "south": "true", "north": "true", "east": "false", "west": "false" },
      "apply": { "model": "metallics:block/iron_mesh_center", "y": 270 }
    },

    {
      "when": { "east": "true", "west": "true", "north": "false", "south": "false" },
      "apply": { "model": "metallics:block/iron_mesh_center_alt", "y": 90 }
    },
    {
      "when": { "west": "true", "east": "true", "north": "false", "south": "false" },
      "apply": { "model": "metallics:block/iron_mesh_center_alt", "y": 270 }
    }
  ]
}
